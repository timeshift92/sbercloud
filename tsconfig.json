{
    "compilerOptions": {
      "baseUrl": ".",
      "module": "commonjs",
      "moduleResolution": "node",
      "target": "es5",
      "noImplicitAny": false,
      "sourceMap": true,
      "outDir": "./dist",
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "declaration": true,
      "paths": {
        "*": ["public/*", "src/*"]
      },
     
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }